<?xml version="1.0" encoding="UTF-8"?>

<section xml:id="c2s2" xmlns:xi="http://www.w3.org/2001/XInclude">
    <title>Prufer Codes</title>

    <exercise workspace="1in">
        <introduction>
            <p>
                Find the Prufer code of the following trees
            </p>
        </introduction>
        <task workspace="1in">
            <statement>
                <p>
                   <figure xml:id="Tree">
                    <caption>pf1</caption>
                    <image width="60%">
  <prefigure xmlns="https://prefigure.org"
             label="prefigurerectangle">
 <diagram dimensions="(300,300)" margins="5">
  <definition>graph={1:[5],2:[4,9],3:[8],4:[5],5:[8],6:[7], 7:[8]}</definition>
  <coordinates bbox="(-1,-1,1,1)">
    <network directed="no" graph="graph" arrows="middle"
             scale="0.8" node-fill="#fcf" node-stroke="black"
	     seed="1" labels="yes"
	     tactile-node-size="40">
    </network>
  </coordinates>
</diagram>
  </prefigure>
</image>
                   </figure> 
                </p>
            </statement>
        </task>
    </exercise>
</section>